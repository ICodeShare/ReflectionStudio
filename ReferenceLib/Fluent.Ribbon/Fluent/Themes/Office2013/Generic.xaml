            <Rectangle x:Name="IndeterminateIcon" Width="7" Height="7" Opacity="0" Fill="#FF666666" />
            <Rectangle x:Name="DisabledVisualElement" Fill="White" Opacity="0" RadiusX="1" RadiusY="1" />
