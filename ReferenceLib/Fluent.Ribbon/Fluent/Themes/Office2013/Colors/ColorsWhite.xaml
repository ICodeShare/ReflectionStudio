                     Color="#FF656D75" />
    <SolidColorBrush x:Key="WindowInnerBorderBrush"
                     Color="White" />
    <LinearGradientBrush x:Key="WindowInnerBackgroundBrush"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FFEBEDF0"
                      Offset="0" />
        <GradientStop Color="#FFE3E6E8"
                      Offset="1" />
    </LinearGradientBrush>
