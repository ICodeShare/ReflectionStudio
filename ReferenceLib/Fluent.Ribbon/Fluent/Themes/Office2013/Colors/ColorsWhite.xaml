                     Color="#808893" />
    <SolidColorBrush x:Key="SliderShadowBrush"
                     Color="White" />
    <SolidColorBrush x:Key="SliderLightenBrush"
                     Color="White" />
    <LinearGradientBrush x:Key="RoundButtonForegroundBrush"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF828588"
                      Offset="0" />
        <GradientStop Color="#FF6A6D70"
                      Offset="1" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ResizeGripFrontBrush"
                     Color="#FF9DA6B0" />
    <SolidColorBrush x:Key="ResizeGripBackBrush"
                     Color="#FFE6E9ED" />

    <SolidColorBrush x:Key="MenuItemBackground"
                     Color="#00000000" />
</ResourceDictionary>